{
    "name": "@grossacasacs/left-phalange",
    "version": "3.0.1",
    "description": "CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML data loader, parser and stringifier",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.cjs",
    "exports": {
        "import": "./src/index.js",
        "require": "./dist/index.cjs",
        "browser": "./dist/browser.es.js",
        "deno": null
    },
    "scripts": {
        "build": "rollup --config",
        "clean": "del-cli dist",
        "lint": "eslint \"**/*.{js,mjs}\"",
        "test": "ava",
        "test-coverage": "nyc ava",
        "try-deno": "deno run examples/deno/test.js",
        "prepublishOnly": "npm t && npm run clean && npm run build"
    },
    "dependencies": {
        "@iarna/toml": "3.0.0",
        "esm": "3.2.25",
        "import-fresh": "3.2.1",
        "ini": "1.3.5",
        "js-yaml": "3.14.0",
        "json5": "2.1.3",
        "parse-json": "5.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "15.1.0",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "ava": "3.13.0",
        "del-cli": "3.0.1",
        "eslint": "7.12.0",
        "nyc": "15.1.0",
        "rollup": "2.32.1",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "temp-write": "4.0.0"
    },
    "ava": {
        "files": [
            "test/*"
        ],
        "require": []
    },
    "nyc": {
        "include": [
            "src/**"
        ],
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "files": [
        "dist",
        "src",
        "license",
        "readme.md",
        "changelog.md"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/GrosSacASac/grossacasacs-left-phalange.git"
    },
    "keywords": [
        "ini",
        "json",
        "json5",
        "toml",
        "yaml",
        "load",
        "parse",
        "stringify"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}
