{
  "name": "@grossacasacs/left-phalange",
  "version": "2.0.1",
  "type": "module",
  "description": "CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML data loader, parser and stringifier",
  "keywords": [
    "ini",
    "json",
    "json5",
    "toml",
    "yaml",
    "load",
    "parse",
    "stringify"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/index.js",
    "browser": null,
    "deno": null
  },
  "files": [
    "dist",
    "src",
    "license",
    "readme.md",
    "changelog.md"
  ],
  "scripts": {
    "build": "rollup --config",
    "clean": "del-cli dist",
    "lint": "eslint \"**/*.{js,mjs}\"",
    "test": "ava",
    "test-coverage": "nyc ava",
    "prepublishOnly": "run-s clean build test"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": "esm"
  },
  "dependencies": {
    "@iarna/toml": "3.0.0",
    "esm": "3.2.25",
    "import-fresh": "3.2.1",
    "ini": "1.3.5",
    "js-yaml": "3.14.0",
    "json5": "2.1.3",
    "parse-json": "5.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "ava": "3.13.0",
    "del-cli": "3.0.1",
    "eslint": "7.12.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "rollup": "2.32.1",
    "temp-write": "4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GrosSacASac/grossacasacs-left-phalange.git"
  }
}
