{
    "name": "@grossacasacs/left-phalange",
    "version": "3.2.9",
    "description": "CommonJS, ESModule, INI, JSON, JSON5, TOML, YAML data loader, parser and stringifier",
    "license": "MIT",
    "type": "module",
    "main": "./dist/index.cjs",
    "exports": {
        "import": "./src/index.js",
        "require": "./dist/index.cjs",
        "browser": "./dist/browser.es.js",
        "deno": "./dist/deno.es.js"
    },
    "scripts": {
        "build-package": "rollup --config",
        "build-ui": "rollup --config ui/rollup.config.js",
        "build": "npm run build-package && npm run build-ui",
        "clean": "del-cli dist",
        "lint-fix": "eslint --ignore-path .gitignore --fix .",
        "lint": "eslint --ignore-path .gitignore .",
        "test": "ava",
        "test-coverage": "nyc ava",
        "try-deno": "deno run examples/deno/test.js",
        "prepublishOnly": "npm t && npm run clean && npm run build",
        "serve": "serve ./ui -p 8080"
    },
    "dependencies": {
        "@iarna/toml": "3.0.0",
        "esm": "3.2.25",
        "import-fresh": "3.2.1",
        "ini": "^2.0.0",
        "js-yaml": "^3.14.1",
        "json5": "2.1.3",
        "parse-json": "5.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "15.1.0",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "ava": "3.13.0",
        "del-cli": "3.0.1",
        "eslint": "^7.15.0",
        "eslint-config-red": "^1.8.1",
        "nyc": "15.1.0",
        "rollup": "2.32.1",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "rollup-plugin-terser": "^7.0.2",
        "serve": "^11.3.2",
        "temp-write": "4.0.0"
    },
    "eslintConfig": {
        "extends": [
            "red"
        ],
        "parserOptions": {
            "ecmaVersion": 2021,
            "sourceType": "module",
            "ecmaFeatures": {}
        },
        "env": {
            "es2021": true,
            "browser": true,
            "node": true
        },
        "globals": {
            "Deno": "readonly"
        }
    },
    "ava": {
        "files": [
            "test/*"
        ],
        "require": []
    },
    "nyc": {
        "include": [
            "src/**"
        ],
        "reporter": [
            "lcov",
            "text"
        ]
    },
    "files": [
        "dist",
        "src",
        "license.txt",
        "readme.md",
        "changelog.md"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/GrosSacASac/grossacasacs-left-phalange.git"
    },
    "keywords": [
        "ini",
        "json",
        "json5",
        "toml",
        "yaml",
        "load",
        "parse",
        "stringify"
    ],
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}
